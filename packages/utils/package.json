{
  "name": "@modern-kit/utils",
  "sideEffects": false,
  "version": "2.7.0",
  "description": "modern-kit/utils",
  "author": "ssi02014 <ssi02014@naver.com>",
  "license": "MIT",
  "bugs": "https://github.com/modern-agile-team/modern-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/modern-agile-team/modern-kit"
  },
  "homepage": "https://modern-agile-team.github.io/modern-kit",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./dist/*": {
      "types": "./dist/*",
      "require": "./dist/*",
      "import": "./dist/*"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "require": "./dist/*/index.cjs",
      "import": "./dist/*/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "vitest --typecheck",
    "test:run": "vitest run --coverage --typecheck",
    "test:bench": "vitest bench",
    "build": "rm -rf dist && rollup -c",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@modern-kit/types": "workspace:^",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/lodash-es": "^4.17.12",
    "@vitest/coverage-istanbul": "^4.0.4",
    "esbuild": "^0.27.0",
    "eslint": "^10.0.0",
    "jsdom": "^27.0.0",
    "lodash-es": "^4.17.21",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.4"
  },
  "engines": {
    "node": ">=24",
    "yarn": ">=4"
  },
  "packageManager": "yarn@4.10.3",
  "keywords": [
    "react",
    "next",
    "utils",
    "component",
    "ui",
    "hooks",
    "custom hooks"
  ],
  "gitHead": "7722785374b897572934e335817ec297ad45a943"
}
