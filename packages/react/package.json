{
  "name": "@modern-kit/react",
  "sideEffects": false,
  "version": "1.27.2",
  "description": "modern-kit/react",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest --typecheck",
    "test:run": "vitest run --coverage --typecheck",
    "build": "rm -rf dist && rollup -c",
    "typecheck": "tsc --noEmit"
  },
  "author": "ssi02014 <ssi02014@naver.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/ua-parser-js": "^0.7.39",
    "@vitest/coverage-istanbul": "^2.1.3",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.24.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.41",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.24.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "typescript-plugin-css-modules": "^5.1.0",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@modern-kit/utils": "workspace:^",
    "@types/lodash-es": "^4.17.12",
    "classnames": "^2.5.1",
    "lodash-es": "^4.17.21",
    "ua-parser-js": "^1.0.39"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "next",
    "utils",
    "component",
    "ui",
    "hooks",
    "custom hooks"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "require": "./dist/components/index.js",
      "import": "./dist/components/index.mjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "require": "./dist/hooks/index.js",
      "import": "./dist/hooks/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "require": "./dist/utils/index.js",
      "import": "./dist/utils/index.mjs"
    },
    "./components/*": {
      "types": "./dist/components/[name]/[name].d.ts",
      "require": "./dist/components/[name]/[name].js",
      "import": "./dist/components/[name]/[name].mjs"
    },
    "./hooks/*": {
      "types": "./dist/hooks/[name]/[name].d.ts",
      "require": "./dist/hooks/[name]/[name].js",
      "import": "./dist/hooks/[name]/[name].mjs"
    },
    "./utils/*": {
      "types": "./dist/utils/[name]/[name].d.ts",
      "require": "./dist/utils/[name]/[name].js",
      "import": "./dist/utils/[name]/[name].mjs"
    },
    "./package.json": "./package.json"
  },
  "gitHead": "f451da4fef108695b343911200ec95ee34da4e97"
}
